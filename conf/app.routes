# microservice specific routes

->          /govuk-frontend        govuk.Routes
->          /hmrc-frontend         hmrcfrontend.Routes
GET         /assets/*file          controllers.Assets.versioned(path = "/public", file: Asset)

GET         /                      controllers.IndexController.onPageLoad
GET        /initialise            controllers.AddressLookupController.initialiseJourney
GET        /callback              controllers.AddressLookupController.callback(id: String)

GET         /hello-world           controllers.HelloWorldController.onLoad
POST        /hello-world           controllers.HelloWorldController.onSubmit

GET         /who-is-disclosing     controllers.UserTypeController.onLoad
POST        /who-is-disclosing     controllers.UserTypeController.onSubmit

GET         /unauthorised          controllers.errors.UnauthorisedController.onPageLoad

GET         /disclaimer            controllers.SupportingDocController.onLoad
POST        /disclaimer            controllers.SupportingDocController.onSubmit